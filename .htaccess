 ErrorDocument 404 http://www.allexlima.com/NotFound

<files ~ "^.*\.([Hh][Tt][Aa])">
	order allow,deny
	deny from all
	satisfy all
</files>

<FilesMatch ".(php|htm|html|css|js)$">
	AddDefaultCharset UTF-8
</FilesMatch>


RewriteEngine On
RewriteRule ^NotFound/?$ 404.html [NC,L]

Options All -Indexes
